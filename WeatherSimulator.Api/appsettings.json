{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "InMemory": "[InMemory DB name]"
  },
  "Settings": {
    "CreationDelaySec": 30,
    "ServerResponseDelayMSec": 3000
  },
  "Seq": {
    "ServerUrl": "[seq server url]",
    "ApiKey": "[seq api key]",
    "MinimumLevel": "Trace",
    "LevelOverride": {
      "Microsoft": "Warning"
    }
  },
  "MokWeatherParams": {
    "lowestTemperature": -40,
