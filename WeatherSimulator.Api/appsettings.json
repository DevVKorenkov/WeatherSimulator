    "middleTemperature": 0,
    "highestWindPower": 100,
    "lowestDate": {
      "year": 2000,
      "month": 1,
      "day": 1
    },
    "Time": {
      "hour": 0,
      "minute": 0,
      "second": 0
    }
  },
  "AllowedHosts": "*"
}
