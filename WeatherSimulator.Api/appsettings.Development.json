{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "InMemory": "weather"
  },
  "Settings": {
    "CreationDelaySec": 30,
    "ServerResponseDelayMSec": 3000
  },
  "Seq": {
    "ServerUrl": "http://localhost:5341/",
    "ApiKey": "1234567890",
    "MinimumLevel": "Trace",
    "LevelOverride": {
      "Microsoft": "Warning"
    }
  },
  "MokWeatherParams": {
    "lowestTemperature": -40,
    "highestTemperature": 40,
    "middleTemperature": 0,
    "highestWindPower": 100,
    "lowestDate": {
      "year": 2000,
      "month": 1,
      "day": 1
    },
    "Time": {
      "hour": 0,
      "minute": 0,
      "second": 0
    }
  },
  "AllowedHosts": "*"
}
